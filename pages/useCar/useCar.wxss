/* pages/useCar/useCar.wxss */
.s_my{
  font-size: 44rpx;
  letter-spacing: 2px;
}
.card_form{
  margin-top: 40rpx;
  display: block;
  position: relative;
}
.formInput{
  border-radius: 10rpx;
  border: 1px solid #16A085;
  padding: 20rpx;
  margin: 30rpx 0;
  font-size: 36rpx;
}
.formInput picker{
  z-index: 111;
}
.formInput textarea{
  width: 100%;
  height: 120rpx;
  z-index: 111;
}
.s_foot{
  width: 100%;
  margin-top: 60rpx;
}
.beizhu{
  position: relative;
}
.bg{
  position: absolute;
  bottom: -50rpx;
  right: -20rpx;
  z-index: 110;
}
.bg image{
  width: 400rpx;
  height: 400rpx;
  opacity: 0.4;
}
.explain{
  color: #999;
  font-size: 30rpx;
  margin-top: -10rpx;
  margin-bottom: 20rpx;
}
.picker{
  height: 56rpx;
  line-height: 56rpx;
}