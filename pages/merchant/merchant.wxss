/* pages/merchant/merchant.wxss */
@import "../../public/font/iconfont.wxss";
@import "../contact/contact.wxss";
.swiper {
  height: 400rpx;
  width: 100%;
}
.swiper image {
  height: 100%;
  width: 100%;
}
.shop_body{
  padding: 10rpx 40rpx;
  font-size: 30rpx;
}
.shop_header{
  padding: 30rpx;
}
.shop_car .s_header{
  margin: 0;
  width: 130rpx;
}
.shop_car .s_header.shop_liang{
  margin-left: 14rpx;
}
.s_my{
  font-size: 44rpx;
}
.shop_left{
  width: 90%
}
.shop_icon{
  padding: 0 30rpx 0; 
  color: #16A085;
  border-radius: 10rpx;
}
.shop_icon .iconfont{
  font-size: 52rpx;
}
.s_useCar{
  width: 90%;
  background: #16A085;
  color: #fff;
  text-align: center;
  padding: 30rpx 0;
  border-radius: 10rpx;
  margin: 60rpx auto 0;
}